  style.
      body{
      background-color: #52b1ea;
      height: 100%;
      display: table-cell;
      vertical-align: middle;
      }
      .container-table100{
      background-color: #52b1ea !important;
      }


body
  .limiter
    .container-table100
      .wrap-table100
        .table100.ver1.m-b-110
          .table100-head
            table
              thead
                tr.row100.head
                  th.cell100.column1 Nombre
                  th.cell100.column2 Inicio
                  th.cell100.column3 Final
                  th.cell100.column4 Duracion
          .table100-body.js-pscroll
            table
              tbody
                  tr.row100.body
                      td.cell100.column1= competition.name
                      td.cell100.column2= competition.ini
                      td.cell100.column3= competition.fin
                      td.cell100.column4= competition.duration
        .table100.ver1.m-b-110
          .table100-head
            table
              thead
                tr.row100.head
                  th.cell100.column1 Nombre del problema
          .table100-body.js-pscroll
            table
              tbody
                each problem in problems
                  tr.row100.body
                      td.cell100.column1= problem.name
